    {{ com top-level type constraint }}
  | default_typing_spec                                         :: :: default
    {{ com default kind and type assumptions }}
  | scattered_def					:: :: scattered
    {{ com scattered definition }} 
  | dec_spec                                            :: :: reg_dec
    {{ com register declaration }}

defs :: '' ::=
  {{ com Definition sequence }}
  | def1  .. defn                 :: :: Defs



